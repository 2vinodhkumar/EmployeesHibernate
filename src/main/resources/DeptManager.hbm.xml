<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="net.erp.model.DeptManager" table="dept_manager">
        <id name="id" column="emp_no">
            <generator class="increment"></generator>
        </id>
        <property name="deptNo" column="dept_no"></property>
        <property name="fromDate" column="from_date"></property>
        <property name="toDate" column="to_date"></property>
    </class>
</hibernate-mapping>